<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="390.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.javafxproject.managerlogin.managercontroller">
   <children>
      <ImageView fx:id="image" fitHeight="390.0" fitWidth="700.0" layoutX="1.0" layoutY="-2.0" pickOnBounds="true" preserveRatio="true" />
      <TableView fx:id="tableview" layoutY="60.0" prefHeight="330.0" prefWidth="701.0" stylesheets="@table.css" />
       <Label layoutX="292.0" layoutY="10.0" prefHeight="40.0" prefWidth="146.0" styleClass="boundary-label" stylesheets="@label.css" text="MY PLAYERS">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <Button layoutX="574.0" layoutY="14.0" mnemonicParsing="false" onAction="#logout" prefHeight="32.0" prefWidth="86.0" stylesheets="@button.css" text="Log Out" textFill="#370fd7">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button layoutX="44.0" layoutY="14.0" mnemonicParsing="false" onAction="#buyplayer" prefHeight="32.0" prefWidth="103.0" stylesheets="@button.css" text="Buy Player" textFill="#2312dc">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
