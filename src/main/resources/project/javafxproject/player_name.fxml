<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="390.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.javafxproject.search_by_player.searchbynamecontroller">
   <children>
      <TextField fx:id="nametosearch" layoutX="44.0" layoutY="25.0" prefHeight="43.0" prefWidth="243.0" promptText="Enter Player Name" stylesheets="@textfield.css" />
      <Button layoutX="350.0" layoutY="25.0" mnemonicParsing="false" onAction="#search" prefHeight="43.0" prefWidth="95.0" stylesheets="@button.css" text="Search">
         <font>
            <Font size="16.0" />
         </font></Button>
      <Button layoutX="471.0" layoutY="25.0" mnemonicParsing="false" onAction="#reset" prefHeight="43.0" prefWidth="95.0" stylesheets="@button.css" text="Reset">
         <font>
            <Font size="16.0" />
         </font></Button>
      <Button layoutX="590.0" layoutY="25.0" mnemonicParsing="false" onAction="#quit" prefHeight="43.0" prefWidth="95.0" stylesheets="@button.css" text="Back">
         <font>
            <Font size="16.0" />
         </font></Button>
      <TableView fx:id="tableView" layoutY="109.0" prefHeight="281.0" prefWidth="700.0" stylesheets="@table.css" />
   </children>
</AnchorPane>
